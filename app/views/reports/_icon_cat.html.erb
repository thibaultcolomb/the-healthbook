
      <% if category == "Blood Tests" %>
        <i class="fa-solid fa-droplet"></i>
      <% elsif (category == "Imaging" || category == "Surgery") %>
        <i class="fa-solid fa-hospital"></i>
      <% elsif category == 'Cardiology' %>
        <i class="fa-solid fa-heart-pulse"></i>
      <% elsif category == 'Pulmonary Function Test (PFT)' %>
        <i class="fa-solid fa-lungs"></i>
      <% elsif category == 'Genetic Testing' %>
        <i class="fa-solid fa-dna"></i>
      <% elsif category == 'Neurological' %>
        <i class="fa-solid fa-brain"></i>
      <% elsif (category == 'Obstetric and Gynecological' || category == "Urology" || category == "Dermatology" || category == "Gastroenterology" || category == 'Endoscopy') %>
        <i class="fa-solid fa-stethoscope"></i>
      <% elsif category == 'Orthopedic' %>
        <i class="fa-solid fa-bone"></i>
      <% elsif category == 'Ophthalmology' %>
        <i class="fa-solid fa-eye"></i>
      <% elsif category == 'Dental' %>
        <i class="fa-solid fa-tooth"></i>
      <% elsif (category == 'Allergy Testing' || category == 'Nuclear Medicine') %>
        <i class="fa-solid fa-vial-virus"></i>
      <% elsif category == 'Pathology' %>
        <i class="fa-solid fa-virus"></i>
      <% elsif %>
        <i class="fa-solid fa-user-doctor"></i>
      <% end %>
